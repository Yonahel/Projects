	<child link="middle1"/>
	<origin xyz="0 0 0" rpy="0 0 0" />
	<axis xyz"0 0 0" rpy="0 0 0" />
      </joint>


      <joint name="base_ring_joint" type="revolute">
	<parent link="base_link"/>
	<child link="ring1"/>
	<origin xyz="0 0 0" rpy="0 0 0" />
	<axis xyz"0 0 0" rpy="0 0 0" />
      </joint>


